@model DataAccessLayer.ServiceRecordInfo
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetServiceRecordInfoByServiceRefNo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

</head>
<body>
    <div class="container">
        <h2>GetServiceRecordInfoByServiceRefNo</h2>
        <div>
            <table class="table table-hover">
                <tr class="table-primary">
                    <th>ServiceRefNo</th>
                    <th>CustId</th>
                    <th>ModelNo</th>
                    <th>ServiceDate/th>
                    <th>NextServiceDate</th>
                    <th>ServiceRepId</th>
                   
                </tr>
                <tr>
                    <td>@Model.ServiceRefNo</td>
                    <td>@Model.CustId</td>
                    <td>@Model.ModelNo</td>
                    <td>@Model.ServiceDate/td>
                    <td>@Model.NextServiceDate</td>
                    <td>@Model.ServiceRepId</td>
                    
                </tr>

            </table>
        </div>
    </div>
</body>
</html>
